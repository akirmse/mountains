	$(POINTLIB) \

FIND_HIGH_POINT_OBJS = \
	$(OUTDIR)/easylogging++.o \
	$(OUTDIR)/filter.o \
	$(OUTDIR)/find_high_point.o \
	$(OUTDIR)/high_point_task.o \
	$(OUTDIR)/peakbagger_point.o \
	$(OUTDIR)/point_map.o \
	$(OUTDIR)/tile.o \
	$(OUTDIR)/tile_cache.o \
	$(POINTLIB) \

all : makedirs $(OUTDIR)/mapper.exe $(OUTDIR)/find_missing_peaks.exe \
	$(OUTDIR)/find_peakbagger_duplicates.exe $(OUTDIR)/isolation.exe $(OUTDIR)/peakbagger_isolation_mapper.exe \
	$(OUTDIR)/prominence.exe $(OUTDIR)/merge_divide_trees.exe $(OUTDIR)/peakbagger_prominence_mapper.exe \
	$(OUTDIR)/loj_prominence_mapper.exe $(OUTDIR)/peaklist_prominence_mapper.exe $(OUTDIR)/filter_points.exe \
	$(OUTDIR)/find_high_point.exe

$(POINTLIB) : $(POINTLIB_OBJS)
	$(AR) /OUT:$@ $**

$(OUTDIR)/find_missing_peaks.exe: $(FIND_MISSING_PEAKS_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/find_peakbagger_duplicates.exe: $(FIND_PEAKBAGGER_DUPLICATES_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/isolation.exe: $(ISOLATION_OBJS)
	$(LINK) $** $(LIBS) -OUT:$@ $(LINKFLAGS)

$(OUTDIR)/peakbagger_isolation_mapper.exe: $(PEAKBAGGER_ISOLATION_MAPPER_OBJS)
	$(LINK) $** $(LIBS) -OUT:$@ $(LINKFLAGS)

$(OUTDIR)/mapper.exe: $(LOJ_MAPPER_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/prominence.exe: $(PROMINENCE_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/merge_divide_trees.exe: $(MERGE_DIVIDE_TREES_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/peakbagger_prominence_mapper.exe: $(PEAKBAGGER_PROMINENCE_MAPPER_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/loj_prominence_mapper.exe: $(LOJ_PROMINENCE_MAPPER_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/peaklist_prominence_mapper.exe: $(PEAKLIST_PROMINENCE_MAPPER_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/filter_points.exe: $(FILTER_POINTS_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

$(OUTDIR)/find_high_point.exe: $(FIND_HIGH_POINT_OBJS)
	$(LINK) $** -OUT:$@ $(LIBS) $(LINKFLAGS)

{$(SOURCEDIR)}.cpp{$(OUTDIR)}.o:
	$(CC) $(CFLAGS) /FpCpch /Fd$(OUTDIR)\vc90.pdb /Fo$@ -c $< 

{$(SOURCEDIR)}.cc{$(OUTDIR)}.o:
	$(CC) $(CFLAGS) /FpCpch /Fd$(OUTDIR)\vc90.pdb /Fo$@ -c $< 

makedirs:
	-@$(MKDIR) $(OUTDIR)

clean:
	@$(RM) $(OUTDIR)\*
	@$(RMDIR) $(OUTDIR)

depend:
	makedepend -Y -p'$(OUTDIR)/' *.cpp
	@$(RM) Makefile.bak

