	$(OUTDIR)/latlng.o \
	$(OUTDIR)/peak_finder.o \
	$(OUTDIR)/peakbagger_collection.o \
	$(OUTDIR)/peakbagger_point.o \
	$(OUTDIR)/point_map.o \
	$(OUTDIR)/tile.o \
	$(OUTDIR)/tile_cache.o \
