	$(OUTDIR)/latlng.o \
	$(OUTDIR)/loj_point.o \
	$(OUTDIR)/peaklist_collection.o \
	$(OUTDIR)/peaklist_prominence_mapper.o \
	$(OUTDIR)/prominence_collection.o \
	$(OUTDIR)/prominence_point.o \
